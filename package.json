{
  "name": "simplestyle-js",
  "version": "2.7.0-alpha.1",
  "description": "An incredibly straightforward and simple CSS-in-JS solution with zero runtime dependencies, and out-of-the-box TypeScript support",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benduran/simplestyle.git"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc --project ./tsconfig.json && yarn prepdirs",
    "coveralls": "run-s test:coverage coverage:pipe",
    "coverage:pipe": "cat ./coverage/lcov.info | coveralls",
    "prepdirs": "mv ./dist/src/* ./dist && rm -rf ./dist/src && rm -rf ./dist/test",
    "lint": "eslint './src/**/*.ts'",
    "lint:fixall": "eslint './src/**/*.ts' --fix",
    "test:coverage": "tsdx test --coverage",
    "test": "jest test",
    "test:watch": "jest test --watch",
    "prepublishOnly": "yarn build",
    "git-pre-commit": "yarn lint && yarn test"
  },
  "keywords": [
    "CSS-in-JS",
    "CSS",
    "Style",
    "Styled",
    "Simple"
  ],
  "author": "Benjamin Duran <stratodyne@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/autoprefixer": "^9.7.2",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@zeit/git-hooks": "^0.1.4",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.2",
    "coveralls": "^3.1.0",
    "eslint-config-react-yas": "^1.2.1",
    "jest": "^27.0.4",
    "jsdom": "^16.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "csstype": "^3.0.8"
  },
  "resolutions": {
    "eslint": "7.28.0"
  }
}
