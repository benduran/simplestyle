{
  "name": "simplestyle-js",
  "version": "1.0.3",
  "description": "An incredibly straightforward and simple CSS-in-JS solution with zero runtime dependencies, and out-of-the-box TypeScript support",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "types": "dist/commonjs/index.d.ts",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benduran/simplestyle.git"
  },
  "scripts": {
    "build:commonjs": "tsc -p ./tsconfig.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "run-s clean build:commonjs build:esm",
    "clean": "rimraf ./dist",
    "test": "jest --env jsdom",
    "test:watch": "jest --env jsdom --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "CSS-in-JS",
    "CSS",
    "Style",
    "Styled",
    "Simple"
  ],
  "author": "Benjamin Duran <stratodyne@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/autoprefixer": "^9.7.1",
    "@types/jest": "^25.1.4",
    "@types/jsdom": "^16.1.0",
    "autoprefixer": "^9.7.4",
    "jest": "^25.1.0",
    "jsdom": "^16.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "csstype": "^2.6.9"
  }
}
