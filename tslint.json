{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "import-name": [false],
    "variable-name": [false],
    "object-literal-sort-keys": [false],
    "jsx-no-multiline-js": [false],
    "align": [false],
    "max-line-length": [true, 180],
    "no-increment-decrement": [false]
  }
}
